{
  "name": "plaax-nodejs",
  "version": "1.0.0",
  "description": "",
  "main": "index.ts",
  "engines": {
    "node": ">=18.12.1 <19.0.0",
    "npm": ">=9.5.0 < 10.0.0"
  },
  "scripts": {
    "lint": "eslint .",
    "lint-fix": "eslint . --fix",
    "lint-debug": "DEBUG=eslint:cli-engine npm run lint",
    "test": "jest",
    "test-coverage": "jest --coverage",
    "ts-check": "npx tsc --noEmit",
    "build": "npx tsc --project ./tsconfig.build.json",
    "dev": "npm run build && NODE_ENV=development node dist/server/start",
    "start": "NODE_ENV=production node dist/server/start",
    "docker-build": "docker build -t gr/plaax-nodejs .",
    "docker-run": "docker run -p 3000:3000 gr/plaax-nodejs",
    "docker-start": "docker container start $(docker container ls -a | awk '/plaax-nodejs/ {print $1}' | head -n1)",
    "docker-stop": "docker container stop $(docker container ls | awk '/plaax-nodejs/ {print $1}')",
    "docker-list-containers": "docker container ls -a | awk '/plaax-nodejs/'",
    "docker-list-images": "docker images | awk '/plaax-nodejs/'",
    "clean": "rm -rf dist coverage",
    "docker-rm-containers": "docker rm $(docker container ls -a | awk '/plaax-nodejs/ {print $1}')",
    "docker-rm-images": "docker rmi $(docker images | awk '/plaax-nodejs/ {print $3}')"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/giacomoratta/plaax-nodejs.git"
  },
  "author": "",
  "license": "GPU 3.0",
  "bugs": {
    "url": "https://github.com/giacomoratta/plaax-nodejs/issues"
  },
  "homepage": "https://github.com/giacomoratta/plaax-nodejs#readme",
  "devDependencies": {
    "@aws-sdk/client-dynamodb": "^3.370.0",
    "@types/aws-lambda": "^8.10.119",
    "@types/jest": "^29.5.3",
    "@types/koa__router": "^12.0.0",
    "@types/node": "^20.4.4",
    "@typescript-eslint/eslint-plugin": "^6.2.0",
    "@typescript-eslint/parser": "^6.2.0",
    "eslint": "^8.45.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^16.0.1",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^29.6.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "@koa/router": "^12.0.0",
    "axios": "1.1.3",
    "dotenv": "^16.0.3",
    "koa": "^2.13.4"
  }
}
